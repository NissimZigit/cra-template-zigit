{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/TemplateNameStyles.scss",
        "story": "templates/component/TemplateName.stories.tsx",
        "index": "templates/component/index.ts"
      },
      "path": "src/Components",
      "withIndex": true,
      "withStyle": true,
      "withTest": false,
      "withStory": true,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateNamePage.tsx",
        "style": "templates/page/TemplateNamePageStyles.scss",
        "index": "templates/page/index.ts"
      },
      "path": "src/Pages",
      "withIndex": true,
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": false
    }
  }
}
